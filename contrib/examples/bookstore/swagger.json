{
  "swagger": "2.0",
  "info": {
    "description": "A simple Bookstore API example.",
    "title": "Bookstore",
    "version": "1.0.0"
  },
  "host": "${MY_PROJECT_ID}.appspot.com",
  "basePath": "/",
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "schemes": [
    "https"
  ],
  "paths": {
    "/shelves": {
      "get": {
        "description": "Returns all shelves in the bookstore.",
        "operationId": "listShelves",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "List of shelves in the bookstore.",
            "schema": {
              "$ref": "#/definitions/listShelvesResponse"
            }
          }
        },
        "security": []
      },
      "post": {
        "description": "Creates a new shelf in the bookstore.",
        "operationId": "createShelf",
        "parameters": [
          {
            "description": "A shelf resource to create.",
            "in": "body",
            "name": "shelf",
            "required": true,
            "schema": {
              "$ref": "#/definitions/shelf"
            }
          }
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "A newly created shelf resource.",
            "schema": {
              "$ref": "#/definitions/shelf"
            }
          }
        }
      }
    },
    "/shelves/{shelf}": {
      "get": {
        "description": "Get a single shelf resource with the given name.",
        "operationId": "getShelf",
        "parameters": [
          {
            "description": "Name of the shelf to get.",
            "format": "int64",
            "in": "path",
            "name": "shelf",
            "required": true,
            "type": "integer"
          }
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "A shelf resource.",
            "schema": {
              "$ref": "#/definitions/shelf"
            }
          }
        },
        "security": []
      },
      "delete": {
        "description": "Deletes a single shelf with the given name.",
        "operationId": "deleteShelf",
        "parameters": [
          {
            "description": "Name of the shelf to delete.",
            "format": "int64",
            "in": "path",
            "name": "shelf",
            "required": true,
            "type": "integer"
          }
        ],
        "responses": {
          "default": {
            "description": "An empty response body."
          }
        }
      }
    },
    "/shelves/{shelf}/books": {
      "get": {
        "description": "Returns all books in a shelf with the given name.",
        "operationId": "listBooks",
        "parameters": [
          {
            "description": "Name of the shelf whose books should be returned.",
            "format": "int64",
            "in": "path",
            "name": "shelf",
            "required": true,
            "type": "integer"
          }
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "List of books on the specified shelf.",
            "schema": {
              "$ref": "#/definitions/listBooksResponse"
            }
          }
        }
      },
      "post": {
        "description": "Creates a new book on the shelf.",
        "operationId": "createBook",
        "parameters": [
          {
            "description": "Name of the shelf where the book should be created.",
            "format": "int64",
            "in": "path",
            "name": "shelf",
            "required": true,
            "type": "integer"
          },
          {
            "description": "Book to create.",
            "in": "body",
            "name": "book",
            "required": true,
            "schema": {
              "$ref": "#/definitions/book"
            }
          }
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "A newly crated book resource.",
            "schema": {
              "$ref": "#/definitions/book"
            }
          }
        }
      }
    },
    "/shelves/{shelf}/books/{book}": {
      "get": {
        "description": "Get a single book with a given name from a shelf.",
        "operationId": "getBook",
        "parameters": [
          {
            "description": "Name of the shelf from which to get the book.",
            "format": "int64",
            "in": "path",
            "name": "shelf",
            "required": true,
            "type": "integer"
          },
          {
            "description": "Name of the book to get from the shelf.",
            "format": "int64",
            "in": "path",
            "name": "book",
            "required": true,
            "type": "integer"
          }
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "A book resource.",
            "schema": {
              "$ref": "#/definitions/book"
            }
          }
        }
      },
      "delete": {
        "description": "Delete a single book with a given name from a shelf.",
        "operationId": "deleteBook",
        "parameters": [
          {
            "description": "Name of the shelf from which to delete the book.",
            "format": "int64",
            "in": "path",
            "name": "shelf",
            "required": true,
            "type": "integer"
          },
          {
            "description": "Name of the book to delete from the shelf.",
            "format": "int64",
            "in": "path",
            "name": "book",
            "required": true,
            "type": "integer"
          }
        ],
        "responses": {
          "default": {
            "description": "An empty response body."
          }
        }
      }
    },
    "/version": {
      "get": {
        "description": "Returns a version of the application.",
        "operationId": "getVersion",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "Version of the application.",
            "schema": {
              "$ref": "#/definitions/version"
            }
          }
        },
        "security": []
      }
    }
  },
  "definitions": {
    "book": {
      "properties": {
        "id": {
          "type": "number"
        },
        "shelf": {
          "type": "number"
        },
        "author": {
          "type": "string"
        },
        "title": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "shelf",
        "author",
        "title"
      ]
    },
    "listBooksResponse": {
      "properties": {
        "books": {
          "items": {
            "$ref": "#/definitions/book"
          },
          "type": "array"
        }
      },
      "required": [
        "books"
      ],
      "type": "object"
    },
    "listShelvesResponse": {
      "properties": {
        "shelves": {
          "items": {
            "$ref": "#/definitions/shelf"
          },
          "type": "array"
        }
      },
      "type": "object"
    },
    "shelf": {
      "properties": {
        "id": {
          "type": "number"
        },
        "theme": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "theme"
      ]
    },
    "version": {
      "properties": {
        "version": {
          "type": "string"
        }
      },
      "required": [
        "version"
      ]
    }
  }
}
